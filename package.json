{
  "name": "authsome",
  "version": "0.0.1",
  "description": "NestJS + Fastify + Prisma 認証バックエンド (メール/パスワード・OAuth・FIDO2/Passkey対応)",
  "author": "nori-k",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gen:jwt-secret": "node -e \"console.log(require('crypto').randomBytes(64).toString('base64'))\"",
    "gen:jwt-secret-env": "node scripts/gen-jwt-secret.js"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.3",
    "@prisma/client": "^6.9.0",
    "@simplewebauthn/server": "^13.1.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "fastify": "^5.4.0",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.28.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.31",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "watchPathIgnorePatterns": [
      "/coverage/",
      "/dist/"
    ],
    "testPathIgnorePatterns": [
      "/coverage/",
      "/dist/"
    ]
  },
  "packageManager": "pnpm@10.11.1",
  "keywords": [
    "nestjs",
    "fastify",
    "prisma",
    "auth",
    "jwt",
    "oauth",
    "passkey",
    "fido2",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nori-k/authsome.git"
  },
  "bugs": {
    "url": "https://github.com/yourname/authsome/issues"
  },
  "homepage": "https://github.com/yourname/authsome#readme",
  "maintainers": [
    "nori-k"
  ],
  "engines": {
    "node": ">=24.0.0 <25.0.0",
    "pnpm": ">=8.0.0 <11.0.0"
  },
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": "./dist/main.js"
  }
}
